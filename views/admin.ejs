<% pagename = 'admin' %>

<h2>Users</h2>

<table id="admin" class="table table-bordered table-striped">
<thead><th>name</th><th>twitter auth</th><th>paypal viewed</th><th>paypal approved</th><th>created date</th></thead>
<tbody>
<% for ( var i=0; i < users.length; i++ ) { %>	
<tr>
	<td><a href="/admin/<%= users[i].username %>"><%= users[i].username %></a></td>
	<td><% if( users[i].token ) { %>true<% } else { %>false<% } %></td>
	<td><% if( users[i].preapproval_key ) { %>true<% } else { %>false<% } %></td>
	<td><% if( users[i].email ) { %>true<% } else { %>false<% } %></td>
	<td><%= users[i].created %></td>	
</tr>
<% } %>
</tbody>
</table>

<h2>rateLimitStatus</h2>

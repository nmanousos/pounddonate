reset_time: <%= rateLimitStatus.reset_time %><br/>
remaining_hits: <%= rateLimitStatus.remaining_hits %><br/>
hourly_limit: <%= rateLimitStatus.hourly_limit %><br/>
reset_time_in_seconds: <%= rateLimitStatus.reset_time_in_seconds %><br/>
photos.daily_limit: <%= rateLimitStatus.photos.daily_limit %><br/>
photos.reset_time: <%= rateLimitStatus.photos.reset_time %><br/>
photos.remaining_hits: <%= rateLimitStatus.photos.remaining_hits %><br/>
photos.reset_time_in_seconds: <%= rateLimitStatus.photos.reset_time_in_seconds %>
</p>

